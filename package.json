{
  "name": "moonlight-web",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "lint": "eslint src",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.0",
    "axios": "^0.27.2",
    "pinia": "^2.0.20",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "^18.7.8",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "c8": "^7.12.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "sass": "^1.54.5",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.22.1",
    "vue-tsc": "^0.40.1"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint"
    ]
  }
}
