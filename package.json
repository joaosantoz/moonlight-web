{
  "name": "moonlight-web",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "lint": "eslint src",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vueuse/core": "^8.6.0",
    "axios": "^0.27.2",
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "c8": "^7.11.3",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "sass": "^1.52.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitest": "^0.14.1",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint"
    ]
  }
}
